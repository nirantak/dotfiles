*.py       text diff=python
*.rb       text diff=ruby
*.rake     text diff=ruby
*_spec.rb  text diff=rspec
*.erb      text diff=html
*.sass     text diff=css
*.scss     text diff=css
*.sh       text eol=lf
*.sql      text
*.ts       text diff=javascript
*.tsx      text diff=html
*.xml      text diff=html
*.xhtml    text diff=html

