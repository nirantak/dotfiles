*.bat      text eol=crlf
*.css      text diff=css
*.htm      text diff=html
*.html     text diff=html
*.go       text diff=golang
*.inc      text
*.ini      text
*.js       text diff=javascript
*.json     text
*.jsx      text diff=html
*.md       text diff=markdown
*.php      text diff=php
