{
  "workbench.colorTheme": "Default Dark+",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.settings.editor": "json",
  "workbench.settings.openDefaultSettings": true,
  "workbench.settings.useSplitJSON": true,
  "workbench.editor.wrapTabs": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.startupEditor": "welcomePageInEmptyWorkbench",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.editor.untitled.hint": "hidden",
  "window.titleBarStyle": "custom",
  "telemetry.telemetryLevel": "off",
  "editor.fontFamily": "Fira Code",
  "editor.fontWeight": "normal",
  "editor.fontLigatures": false,
  "editor.fontSize": 14,
  "editor.wordWrap": "on",
  "editor.renderWhitespace": "boundary",
  "editor.tabSize": 2,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorSurroundingLines": 5,
  "editor.snippetSuggestions": "top",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.rulers": [80],
  "editor.parameterHints.enabled": false,
  "editor.suggestSelection": "first",
  "editor.suggest.showStatusBar": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.definitionLinkOpensInPeek": true,
  "editor.padding.bottom": 10,
  "editor.padding.top": 10,
  "editor.largeFileOptimizations": false,
  "editor.renderLineHighlight": "all",
  "editor.inlineSuggest.enabled": true,
  "editor.tabCompletion": "on",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.experimental.stickyScroll.enabled": true,
  "explorer.confirmDelete": false,
  "explorer.incrementalNaming": "smart",
  "breadcrumbs.enabled": true,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "search.searchEditor.doubleClickBehaviour": "openLocationToSide",
  "search.searchEditor.defaultNumberOfContextLines": 5,
  "search.searchEditor.reusePriorSearchConfiguration": true,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "'MesloLGS-NF-Regular', 'Andale Mono', monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.enableBell": true,
  "terminal.external.osxExec": "iTerm.app",
  "files.autoSave": "onWindowChange",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.restoreUndoStack": false,
  "files.defaultLanguage": "plaintext",
  "files.associations": {
    "*.lock": "json",
    "*.json.*": "json",
    "*.yml.*": "yaml",
    "*.yaml.*": "yaml",
    ".ansible-lint": "yaml",
    "Pipfile": "toml",
    "*.env": "shellscript",
    ".env.*": "shellscript",
    "*.dat": "CSV (pipe)",
    ".slugignore": "ignore",
    ".flake8": "ini",
    "*.cnf": "ini",
    "*.conf.*": "properties",
    "Dockerfile*": "dockerfile",
    "*.scpt": "shellscript",
    "*.scp": "shellscript",
    "run": "shellscript",
    "hosts": "ini",
    "*.plist": "xml",
    "requirements*.txt": "pip-requirements",
    ".coveragerc": "ini",
    "_headers": "plaintext",
    "_redirects": "plaintext"
  },
  "git.autofetch": true,
  "git.autoStash": true,
  "git.branchWhitespaceChar": "_",
  "git.enableCommitSigning": true,
  "git.confirmSync": false,
  "git.mergeEditor": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.venv": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/*.venv/**": true,
    "**/.hg/store/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.venv": true,
    "**/.git": true,
    "**/*.code-search": true
  },
  // Language Settings
  "python.pythonPath": "python",
  "python.languageServer": "Pylance",
  "python.linting.maxNumberOfProblems": 50,
  "python.formatting.provider": "black",
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.ignorePatterns": [
    ".vscode/*.py",
    "**/site-packages/**/*.py",
    ".git/**",
    "**/__pycache__/**",
    "**/node_modules/**"
  ],
  "[python]": {
    "editor.tabSize": 4,
    "editor.rulers": [80, 120],
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "editor.defaultFormatter": "ms-python.python",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[ruby]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.defaultFormatter": "mbessey.vscode-rufo"
  },
  "[html]": {
    "editor.formatOnSave": false
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "[plaintext]": {
    "editor.defaultFormatter": null
  },
  "go.toolsManagement.autoUpdate": true,
  // Plugin Settings
  "material-icon-theme.activeIconPack": "none",
  "emmet.excludeLanguages": [],
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.syntaxProfiles": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "indentRainbow.colors": [
    "rgba(32,64,32,0.3)",
    "rgba(64,32,64,0.3)",
    "rgba(16,48,48,0.3)",
    "rgba(64,64,16,0.3)"
  ],
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.userWords": [
    "Nirantak",
    "Raghav",
    "httpx",
    "dotenv",
    "asyncio",
    "wsiwn",
    "typer"
  ],
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "json": false,
    "plaintext": true,
    "markdown": true
  }
}
